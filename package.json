{
  "name": "vue-at",
  "description": "At.js for Vue",
  "version": "2.5.0-beta.1",
  "author": "Fritz Lin <uxfritz@163.com>",
  "repository": "https://github.com/fritx/vue-at",
  "scripts": {
    "test": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text mocha-webpack --webpack-config webpack/demo.js --require test/setup.js test/**/*.spec.js",
    "dev": "webpack-dev-server --config webpack/demo --open --inline --hot",
    "demo": "webpack --config webpack/demo --progress --hide-modules",
    "build": "webpack --config webpack/prod --progress --hide-modules",
    "prepublish": "npm run build"
  },
  "main": "dist/vue-at.js",
  "dependencies": {},
  "peerDependencies": {
    "vue": "2.x"
  },
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.12",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-preset-env": "^1.6.0",
    "expect": "^21.2.1",
    "nyc": "^11.4.1",
    "jsdom": "^11.3.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^3.5.3",
    "mocha-webpack": "^1.0.0-rc.1",
    "webpack-node-externals": "^1.6.0",
    "babel-core": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-stage-0": "^6.16.0",
    "cross-env": "^3.0.0",
    "css-loader": "^0.25.0",
    "file-loader": "^0.9.0",
    "node-sass": "^4.0.0",
    "sass-loader": "^4.1.0",
    "textarea-caret": "^3.0.2",
    "uglifyjs-webpack-plugin": "^1.2.2",
    "vue": "^2.1.0",
    "vue-loader": "^10.3.0",
    "vue-style-loader": "^1.0.0",
    "vue-template-compiler": "^2.2.4",
    "webpack": "^2.1.0-beta.25",
    "webpack-dev-server": "^2.1.0-beta.9"
  },
  "nyc": {
    "include": [
      "src/**/*.(js|vue)"
    ],
    "instrument": false,
    "sourceMap": false
  }
}
